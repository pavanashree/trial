1.Consecutive

#include <iostream>
using namespace std;

int Consecutive(int arr[],int len) { 

  // code goes here
  int i,j,track_dif=0,count=0;
  //sort the array
   for(i=0;i<(len-1);i++)
  {
    for(j=0;j<(len-i-1);j++)
    {
      if(arr[j]<arr[j+1])
      {
        int temp = arr[j];
		arr[j] = arr[j+1];
        arr[j+1] = temp;
	  }
    }
  }
  //check for gaps
  for(int i=0;i<(len-1);i++)
  {
    track_dif = arr[i];
	while((arr[i+1]-track_dif)!=1)
	{
	  count++;
	  track_diff++;
	}
  }
  return count; 
            
}

int main() { 
   
  // keep this function call here
  /* Note: In C++ you first have to initialize an array and set 
     it equal to the stdin to test your code with arrays. 
     To see how to enter arrays as arguments in C++ scroll down */
     
  int A[] = gets(stdin);
  int len = sizeof(A)/sizeof(A[0]);
  cout << Consecutive(A,len);
  return 0;
    
} 

***************************************************************************************************
Triple Double

#include <iostream>
using namespace std;

int TripleDouble(int num1, int num2) { 

  // code goes here  
  int arr1[25],arr2[25],i=0,len1=0,len2=0,match_num=0;
  //put num1 to an array
  while(num1!=0)
  {
    arr1[i++] = num1%10;
	len1++;
	num1 = num1/10;
  }
  i=0;
  //put num2 to an array
  while(num2!=0)
  {
    arr2[i++] = num2%10;
	len2++;
	num2 = num2/10;
  }
  
  //check for triple number
  for(i=0;i<(len1-2);i++)
  {
	if((arr1[i]==arr1[i+1]) && (arr1[i]==arr1[i+2]))
	{
		match_num = arr1[i];
		break;
	}
  }
  
  //check for double number if triple number available  
  if(match_num)
  {
	for(i=0;i<(len2-2);i++)
	{
	  if((match_num==arr2[i]) && (arr2[i]==arr2[i+1]))
	  {
		return 1;
	  }
    }
  }
  return 0; 
            
}

int main() { 
  
  // keep this function call here
  cout << TripleDouble(gets(stdin));
  return 0;
    
} 
