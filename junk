6 Alpahabet Run Enryption

#include <iostream>
using namespace std;

string AlphabetRunEncryption(string str) { 

  // code goes here 
  int i=0,start=0,count=0,end=0;
  string start_ch,end_ch;
  //cout<<"Original string "<<str;  
  //Normal erase
  for(i=0;i<(str.length()-1);i++)
  {     
    start=i;
    start_ch = str[i]-1;	    
    while(str[i]+1 == str[i+1])
	{    
      //cout<<"enter";
      if((i+2)<str.length() && str[i+2]=='N')
        break;
	  count++;      
	  i++;            
	}
    end=i;
	end_ch=str[i]+1;
    //cout<<start<<".."<<count<<".."<<end<<".";
	if(count>2)
	{     
	  str.replace(start,count,start_ch);
      //cout<<" here "<<str;      
	  str.replace(end-count+1,1,end_ch);
      i=(end-count+1);
      count=0;
      //cout<<i<<".";
      //cout<<" here "<<str;
	}
    // if(str[i]=='N' || str[i]=='S' || str[i]=='L' || str[i]=='R')
  }
  for(i=0;i<(str.length()-1);i++)
    if(str[i]==str[i+1])
	  str.erase(i,1);
  //cout<<"after one loop "<<str;
  
  
  
  //Eliminate all special symbols.
  for(i=0;i<str.length();i++)
  {
    string ch;
    switch(str[i])
	{      
      case 'N': ch = str[i-1];
                str.replace(i,1,ch);                
              break;
	  case 'S': if(str[i+3] == 'S')
			  {	
			    str.erase(i-1,2);
				i++;
			  }
			  else
			    str.erase(i,1);		      
			  break;
	  case 'L': ch = str[i-1]-1;
              str.replace(i,1,ch);
              ch = str[i-1]+1;
			  str.replace(i-1,1,ch);
			  break;
	  case 'R': ch = str[i-1]+1;
              str.replace(i,1,ch);
              ch = str[i-1]-1;
			  str.replace(i-1,1,ch);	          
			  break;
	}
  }
  
  
  return str;            
}

int main() { 
  
  // keep this function call here
  cout << AlphabetRunEncryption(gets(stdin));
  return 0;
}  

